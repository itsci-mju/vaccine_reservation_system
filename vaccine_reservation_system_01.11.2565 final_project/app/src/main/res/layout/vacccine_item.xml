<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/vaccine_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="15dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="20dp"
    android:orientation="horizontal"
    android:outlineAmbientShadowColor="@color/black"
    android:outlineSpotShadowColor="@color/black"
    app:cardCornerRadius="5dp"
    app:cardElevation="1dp"
    app:layout_anchorGravity="center">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:background="@drawable/common_google_signin_btn_icon_dark_normal_background"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/layoutvaccine_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|center_vertical"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/vaccine_lots"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:background="#FFFFFF"
                app:srcCompat="@drawable/list" />

            <TextView
                android:id="@+id/txtedit_Vdate_Input"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:fontFamily="@font/prompt_medium"
                android:text="วัคซีน"
                android:textColor="@color/black"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txtsubdistrict"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_weight="0"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/VaccineName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/prompt_medium"
                android:text="subdistrict"
                android:textColor="#000000"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/txtsubdistrict4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_weight="0"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/txtedit_Vdate_exp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="@font/prompt_medium"
                    android:paddingLeft="10dp"
                    android:text="วันที่นำเข้า"
                    android:textAlignment="textStart"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Vac_Date_Input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/prompt_medium"
                    android:paddingLeft="10dp"
                    android:text="Date"
                    android:textAlignment="textStart"
                    android:textColor="#F44336"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView48"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:fontFamily="@font/prompt_medium"
                    android:text="บริษัทผู้นำเข้า"
                    android:textAlignment="viewStart"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Company"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/prompt_medium"
                    android:text="newpatient"
                    android:textAlignment="textStart"
                    android:textColor="#F44336"
                    android:textSize="18sp" />

            </LinearLayout>


        </LinearLayout>

        <TextView
            android:id="@+id/textView65"
            android:layout_width="match_parent"
            android:layout_height="9dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:gravity="center|right"
            android:orientation="horizontal"
            android:textAlignment="center">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginRight="-55dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/txtedit"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="right|center_horizontal|center_vertical"
                    android:layout_marginBottom="3dp"
                    android:radius="100dp"
                    app:srcCompat="@drawable/edite_vaccinelist" />

                <ImageView
                    android:id="@+id/txtdel"
                    android:layout_width="50dp"
                    android:layout_height="60dp"
                    android:layout_gravity="right|center_horizontal|center_vertical"
                    android:layout_marginStart="35dp"
                    android:layout_marginTop="-5dp"
                    android:layout_marginEnd="80dp"
                    android:radius="100dp"
                    app:srcCompat="@drawable/trash" />
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/textView66"
            android:layout_width="match_parent"
            android:layout_height="12dp"
            android:layout_marginTop="5dp" />


    </LinearLayout>


</androidx.cardview.widget.CardView>